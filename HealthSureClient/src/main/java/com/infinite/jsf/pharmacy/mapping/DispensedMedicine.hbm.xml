<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.infinite.jsf.pharmacy.model.DispensedMedicine" table="dispensed_medicine">

    <id name="dispenseId" column="dispense_id" type="string">
      <generator class="assigned"/>
    </id>
    <many-to-one name="prescription" class="com.infinite.jsf.provider.model.Prescription" column="prescription_id" not-null="true" fetch="join"/>
    <many-to-one name="doctor" class="com.infinite.jsf.provider.model.Doctor" column="doctor_id" not-null="true" fetch="join"/>
    <many-to-one name="recipient" class="com.infinite.jsf.recipient.model.Recipient" column="h_id" not-null="true" fetch="join"/>
    <property name="quantityDispensed" column="quantity_dispensed" type="integer" not-null="true"/>
    <property name="dispenseDate" column="dispense_date" type="date" not-null="true"/>
  </class>
</hibernate-mapping>
